{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp -R src/proto dist/proto",
    "postbuild": "cp -R src/proto dist/proto",
    "start": "./node_modules/.bin/dotenv -e .env -- node dist/index.js",
    "dev": "./node_modules/.bin/dotenv -e .env -- npx tsx src/index.ts",
    "codegen": "./node_modules/.bin/graphql-codegen --config codegen.yml",
    "test": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/dotenv -e .env -- jest --detectOpenHandles"
  },
  "dependencies": {
    "@fastify/websocket": "^11.0.2",
    "@grpc/grpc-js": "^1.12.6",
    "@grpc/proto-loader": "^0.7.13",
    "@kinetic/db": "workspace:*",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "graphql": "^16.10.0",
    "grpc-tools": "^1.13.0",
    "jest": "29.7.0",
    "mercurius": "^16.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.4",
    "@graphql-codegen/typescript-resolvers": "^4.3.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^8.0.0",
    "jest-esbuild": "^0.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
